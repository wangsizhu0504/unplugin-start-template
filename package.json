{
  "name": "unplugin-start-template",
  "version": "0.1.0",
  "description": "unplugin template",
  "packageManager": "pnpm@7.14.0",
  "keywords": [
    "unplugin",
    "rollup",
    "vite",
    "esbuild",
    "webpack"
  ],

  "author": "kriszu",
  "license": "MIT",
  "homepage": "https://github.com/sxzz/unplugin-starter#readme",
  "bugs": {
    "url": "https://github.com/sxzz/unplugin-starter/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sxzz/unplugin-starter.git"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./vite": {
      "require": "./dist/vite.js",
      "import": "./dist/vite.mjs"
    },
    "./webpack": {
      "require": "./dist/webpack.js",
      "import": "./dist/webpack.mjs"
    },
    "./rollup": {
      "require": "./dist/rollup.js",
      "import": "./dist/rollup.mjs"
    },
    "./esbuild": {
      "require": "./dist/esbuild.js",
      "import": "./dist/esbuild.mjs"
    },
    "./cores": {
      "require": "./dist/cores.js",
      "import": "./dist/cores.mjs"
    },
    "./*": "./*"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx,.json,.md",
    "lint:fix": "pnpm run lint --fix",
    "build": "tsup && tsx scripts/postbuild.mts",
    "dev": "tsup --watch",
    "test": "vitest",
    "release": "bumpp && pnpm publish",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "@rollup/pluginutils": "^5.0.2",
    "magic-string": "^0.26.7",
    "unplugin": "^0.10.1"
  },
  "devDependencies": {
    "@kriszu/eslint-config": "^0.1.3",
    "@types/node": "*",
    "bumpp": "^8.2.1",
    "eslint": "^8.26.0",
    "fast-glob": "^3.2.12",
    "tsup": "^6.3.0",
    "tsx": "^3.11.0",
    "typescript": "^4.8.4",
    "vite": "^3.1.8",
    "vitest": "^0.24.3"
  },
  "engines": {
    "node": ">=14.19.0"
  }
}
